{
    "env": {
        "development": {
            "plugins": [
                "react-hot-loader/babel",
                [
                    "import",
                    {
                        "libraryName": "antd",
                        "style": true
                    }
                ]
            ]
        },
        "production" : {
            "plugins": [
                [
                    "import",
                    {
                        "libraryName": "antd",
                        "style": true
                    }
                ]
            ]
        }
    },
    "presets": [
        [
            "es2015",
            {
                "modules": false
            }
        ],
        "react",
        "stage-0"
    ]
}
